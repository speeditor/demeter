{
    "name": "demeter",
    "version": "0.8.5",
    "description": "Oasis design wrapper and userscripting library for FANDOM Discussions.",
    "homepage_url": "https://github.com/speeditor/demeter",
    "author": "Speedit <speeditwikia@gmail.com>",
    "content_scripts": [{
        "js": ["ext.js"],
        "matches": ["*://*.wikia.com/d/*"]
    }],
    "icons": {
        "16":  "dist/icon-16.png",
        "19":  "dist/icon-19.png",
        "38":  "dist/icon-38.png",
        "48":  "dist/icon-48.png",
        "128": "dist/icon-128.png"
    },
    "permissions": [
        "*://*.github.com/*",
        "*://*.rawgit.com/*",
        "*://*.wikia.com/*"
    ],
    "manifest_version": 2
}